###  https://w3id.org/gfo/light/Attributive
gfo-light:Attributive rdf:type owl:Class .


###  https://w3id.org/gfo/light/Event
gfo-light:Event rdf:type owl:Class .


###  https://w3id.org/gfo/light/Process
gfo-light:Process rdf:type owl:Class .


###  https://w3id.org/gfo/light/Situation
gfo-light:Situation rdf:type owl:Class .


###  https://w3id.org/gfo/light/TimeBoundary
gfo-light:TimeBoundary rdf:type owl:Class .


###  https://w3id.org/rio/Adverse_event
rio:Adverse_event rdf:type owl:Class ;
                  rdfs:subClassOf gfo-light:Event ;
                  rdfs:label "Adverse event"@en ;
                  skos:definition "An adverse event is any untoward/undesirable occurrence in a treatment."@en ;
                  skos:example "Dura mater infection"@en .


###  https://w3id.org/rio/KPI
rio:KPI rdf:type owl:Class ;
        rdfs:subClassOf gfo-light:Attributive ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty gfo-light:partOf ;
                          owl:someValuesFrom rio:Treatment_situation
                        ] ;
        rdfs:label "KPI"@en ;
        skos:altLabel "Key Performance Indicator"@en ;
        skos:definition "Key Performance Indicators (KPIs) are properties (attributives) of the treatment process and its participants (such as doctors, medical instruments and patients) that are relevant for the estimation of risk."@en ;
        skos:example "Age"@en ,
                     "Presence of antibiotic prevention"@en ,
                     "Presence of menginitis vaccination"@en .


###  https://w3id.org/rio/Point_in_time_of_risk_detection
rio:Point_in_time_of_risk_detection rdf:type owl:Class ;
                                    rdfs:subClassOf rio:Point_in_time_of_treatment ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty gfo-light:pointInTimeOf ;
                                                      owl:someValuesFrom rio:Treatment_phase
                                                    ] ;
                                    rdfs:label "Point in time of risk detection"@en ;
                                    skos:altLabel "Risk detection time point"@en ;
                                    skos:definition "A point in time of risk detection is a point in time of treatment at which the current treatment situation (potential risk situation) is analyzed for the presence of risks."@en .


###  https://w3id.org/rio/Point_in_time_of_treatment
rio:Point_in_time_of_treatment rdf:type owl:Class ;
                               rdfs:subClassOf gfo-light:TimeBoundary ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty gfo-light:pointInTimeOf ;
                                                 owl:someValuesFrom rio:Treatment
                                               ] ;
                               rdfs:label "Point in time of treatment"@en ;
                               skos:altLabel "Treatment time point"@en .


###  https://w3id.org/rio/Risk_situation
rio:Risk_situation rdf:type owl:Class ;
                   rdfs:subClassOf rio:Treatment_situation ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:causes ;
                                     owl:someValuesFrom rio:Adverse_event
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:existsIn ;
                                     owl:someValuesFrom rio:Treatment_phase
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:hasPointInTime ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass rio:Point_in_time_of_risk_detection
                                   ] ;
                   rdfs:label "Risk situation"@en ;
                   skos:altLabel "Risk"@en ;
                   skos:definition "A risk situation is a treatment situation that contains such a combination of KPIs that can lead to an adverse event with a certain probability."@en ;
                   skos:example "Dura mater infection risk (situation) during cochlear implantation: age < 5 month AND no meningitis vaccination AND no antibiotic prevention"@en .


###  https://w3id.org/rio/Treatment
rio:Treatment rdf:type owl:Class ;
              rdfs:subClassOf gfo-light:Process ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gfo-light:hasPointInTime ;
                                owl:someValuesFrom rio:Point_in_time_of_treatment
                              ] ;
              rdfs:label "Treatment"@en ;
              skos:definition "A treatment is a process of medical treating a patient in hospital."@en .


###  https://w3id.org/rio/Treatment_phase
rio:Treatment_phase rdf:type owl:Class ;
                    rdfs:subClassOf gfo-light:Process ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty gfo-light:partOf ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass rio:Treatment
                                    ] ;
                    rdfs:label "Treatment phase"@en ;
                    skos:definition "A treatment phase is a time-limited part of the treatment."@en ;
                    skos:example "Anamnesis phase"@en ,
                                 "Indication phase"@en ,
                                 "Intraoperative phase"@en .


###  https://w3id.org/rio/Treatment_situation
rio:Treatment_situation rdf:type owl:Class ;
                        rdfs:subClassOf gfo-light:Situation ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty gfo-light:hasPart ;
                                          owl:someValuesFrom rio:KPI
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty gfo-light:existsIn ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass rio:Treatment
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty gfo-light:hasPointInTime ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass rio:Point_in_time_of_treatment
                                        ] ;
                        rdfs:comment "Treatment situations can be analyzed for the presence of risks at certain points in time (of risk detection)."@en ;
                        rdfs:label "Treatment situation"@en ;
                        skos:definition "A treatment situation is any situation arising during treatment."@en .


